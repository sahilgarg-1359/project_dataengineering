{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adls-projectdp203"
		},
		"ls_datapopulatiobyage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_datapopulatiobyage'"
		},
		"ls_populatiobyage_gen2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_populatiobyage_gen2'"
		},
		"ls_ecdcdatahttp_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{linkedService().SourceBaseUrl}"
		},
		"ls_populatiobyage_gen2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://projectstoragedlgen2.dfs.core.windows.net/"
		},
		"tr_deleteSourceFile_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/57f2810f-ad2b-4589-ae35-d733aba0a4c5/resourceGroups/DataEngineeringDp-203/providers/Microsoft.Storage/storageAccounts/projectstoragedlgen2"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adls-projectdp203_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_datapopulatiobyage_connectionString": {
						"value": "[parameters('ls_datapopulatiobyage_connectionString')]"
					},
					"ls_populatiobyage_gen2_accountKey": {
						"value": "[parameters('ls_populatiobyage_gen2_accountKey')]"
					},
					"ls_ecdcdatahttp_properties_typeProperties_url": {
						"value": "[parameters('ls_ecdcdatahttp_properties_typeProperties_url')]"
					},
					"ls_populatiobyage_gen2_properties_typeProperties_url": {
						"value": "[parameters('ls_populatiobyage_gen2_properties_typeProperties_url')]"
					},
					"tr_deleteSourceFile_properties_typeProperties_scope": {
						"value": "[parameters('tr_deleteSourceFile_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}